{
  "name": "generate-tree",
  "version": "1.0.0",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|md)\"",
    "lint": "eslint ./src/*.ts",
    "lint:fix": "eslint --fix .",
    "start": "cross-env NODE_ENV=dev nodemon --ignore \"src/component-tree/**/*.*\" --exec ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage --colors"
  },
  "author": "Onur Ertugral",
  "homepage": "https://github.com/OnurErtugral/generate-tree",
  "repository": "github:OnurErtugral/generate-tree",
  "bugs": "https://github.com/OnurErtugral/generate-tree/issues",
  "keywords": [
    "tree",
    "cli",
    "visualization",
    "file",
    "directory"
  ],
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format"
    }
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "cross-env": "^7.0.2",
    "fs-extra": "^9.0.0",
    "minimist": "^1.2.5",
    "open": "^7.0.3",
    "ora": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.2.1",
    "@types/minimist": "^1.2.0",
    "@types/node": "^13.13.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-jest": "^25.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint": "^6.8.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.7",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  }
}
